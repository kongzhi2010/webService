<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="demoSqlMap">

	<select id="queryCount" parameterClass="java.lang.String"
		resultClass="java.lang.Integer">
		select
		count(a.id)
		from
		tour a
		where
		1=1
		<isNotEmpty property="tourId">
			and a.tour_id = #tourId#
		</isNotEmpty>
	</select>


</sqlMap>